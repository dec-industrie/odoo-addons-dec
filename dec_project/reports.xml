<openerp>
	<data>
    <record id="project2_report_id" model="ir.actions.report.xml">
        <field name="name">Sale Quotation Report</field>
        <field name="type">ir.actions.report.xml</field>
        <field name="model">sale.order</field>
        <field name="report_name">sale_quot_report_sample</field>
        <field name="report_type">aeroo</field>
        <field name="in_format">oo-odt</field>
        <field name="report_rml">dec_project/report/quote.odt</field>
        <field name="parser_state">default</field>
        <field name="tml_source">file</field>
    </record>

    <ir_set>
        <field eval="'action'" name="key"/>
        <field eval="'client_print_multi'" name="key2"/>
        <field eval="['sale.order']" name="models"/>
        <field name="name">Sale Quotation Report</field>
        <field eval="'ir.actions.report.xml,'+str(project2_report_id)" name="value"/>
        <field eval="True" name="isobject"/>
        <field eval="True" name="replace"/>
    </ir_set>
    
