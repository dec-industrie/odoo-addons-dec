#. module: res_users_signature
#: model:res.users.signature.template,body_html:res_users_signature.user_signature_template
msgid "<table style=\"width: 510px;\" width=\"510\" cellspacing=\"0\" cellpadding=\"0\" border=\"0\">\n"
"                <tbody>\n"
"                  <tr>\n"
"                  <td style=\"vertical-align: top; padding-right: 12px; border-right: 5px solid #404040; width: 120.0px;\" width=\"120.0\" valign=\"top\">\n"
"                    <table cellspacing=\"0\" cellpadding=\"0\" border=\"0\" align=\"center\">\n"
"                    <tbody>\n"
"                      <tr>\n"
"                      <td style=\"width: 120.0px;\" width=\"120.0\"> <img src=\"/logo.png?company=${company.id}\" style=\"width: 120.0px; height: 120.0px; border-radius: 50%; -webkit-border-radius: 50%; -moz-border-radius: 50%;\" width=\"120.0\" height=\"120.0\"/> </td>\n"
"                      </tr>\n"
"                    </tbody>\n"
"                    </table>\n"
"                  </td>\n"
"                  <td style=\"vertical-align: top; padding-right: 7px; padding-left: 12px;\" valign=\"top\">\n"
"                    <table style=\"width: 100%;\" width=\"100%\" cellspacing=\"0\" cellpadding=\"0\" border=\"0\">\n"
"                    <tbody>\n"
"                      <tr>\n"
"                      <td style=\"padding-bottom: 12px;\">\n"
"                        <table style=\"padding-left: 2px; font-weight: normal; width: 100%;\" width=\"100%\" cellspacing=\"0\" cellpadding=\"0\" border=\"0\">\n"
"                        <tbody>\n"
"                          <tr>\n"
"                          <td style=\"font-size: 11px; line-height: 21px; color: #4e4b4c;\">\n"
"                            <span style=\"color: #404040; padding: 0px; margin: 0px; font-size: 16px; font-weight: bold; font-family: Arial, Helvetica, sans-serif;font-size:16px;\">${firstname} ${lastname}\n"
"                            </span>\n"
"                            <br/>\n"
"                            <span style=\"font-family: Arial, Helvetica, sans-serif;font-size:14px;\">${job_title} | </span><span style=\"font-family: Arial, Helvetica, sans-serif;color:#404040;font-size:14px;\">DEC</span>\n"
"                            <br/>\n"
"                            <br/>\n"
"                            <span style=\"color:#404040; font-weight: bold;font-family: Arial, Helvetica, sans-serif;\">A </span><span style=\"text-decoration: none !important; color: #4e4b4c;font-family: Arial, Helvetica, sans-serif;\">${street1} ${street2} ${zip} ${city.upper()}</span>\n"
"                            <br/>\n"
"                            % if phone:\n"
"                            <span style=\"color:#404040; font-weight: bold;font-family: Arial, Helvetica, sans-serif;\">M </span><a href=\"tel:${phone_callable}\" style=\"text-decoration: underline !important; color: #4e4b4c;font-family: Arial, Helvetica, sans-serif;\" target=\"_blank\">${phone}</a> <i style=\"color: #FFF;\"> ­</i>\n"
"                            % endif\n"
"                            <span style=\"color:#404040; font-weight: bold;font-family: Arial, Helvetica, sans-serif;\">T </span><a href=\"tel:${company_phone_callable}\" style=\"text-decoration: underline !important; color: #4e4b4c;font-family: Arial, Helvetica, sans-serif;\" target=\"_blank\">${company_phone} ext ${phone_ext}</a> <i style=\"color: #FFF;\"> ­</i>\n"
"                            <span style=\"color:#404040; font-weight: bold;font-family: Arial, Helvetica, sans-serif;\">F </span><a href=\"tel:${company_fax_callable}\" style=\"text-decoration: underline !important; color: #4e4b4c;font-family: Arial, Helvetica, sans-serif;\" target=\"_blank\">${company_fax}</a> <i style=\"color: #FFF;\"> ­</i>\n"
"                            \n"
"                            % for email in emails:\n"
"                            <span style=\"color:#404040; font-weight: bold;font-family: Arial, Helvetica, sans-serif;\">E </span><a href=\"mailto:${email}\" style=\"text-decoration: underline !important; color: #4e4b4c;font-family: Arial, Helvetica, sans-serif;\" target=\"_blank\">${email}</a><i style=\"color: #FFF;\"> ­</i>\n"
"                            % endfor\n"
"                            % for website in websites:\n"
"                            <span style=\"color:#404040; font-weight: bold;font-family: Arial, Helvetica, sans-serif;\">W </span><a href=\"${website}\" style=\"text-decoration: underline !important; color: #4e4b4c;font-family: Arial, Helvetica, sans-serif;\" target=\"_blank\">${website.lstrip('http://').lstrip('https://')}</a><i style=\"color: #FFF;\"> ­</i>\n"
"                            % endfor\n"
"                          </td>\n"
"                          </tr>\n"
"                        </tbody>\n"
"                        </table>\n"
"                      </td>\n"
"                      </tr>\n"
"                      % if user.signature_social_buttons:\n"
"                      <tr>\n"
"                        <td style=\"padding-top: 12px\"> \n"
"                          <table cellspacing=\"0\" cellpadding=\"0\" border=\"0\">\n"
"                            <tbody>\n"
"                              <tr>\n"
"                                % if company.social_facebook:\n"
"                                <td style=\"padding-right: 4px;\"><a href=\"${company.social_facebook}\" style=\"text-decoration:none;\" target=\"_blank\">\n"
"                                  <img src=\"/res_users_signature/static/img/social_facebook.jpg\" alt=\"facebook\" style=\"height: 24px; width: 24px; border-radius: 4px; -webkit-border-radius: 4px; -moz-border-radius: 4px;\" width=\"24\" height=\"24\"/></a>\n"
"                                </td>\n"
"                                % endif\n"
"                                % if company.social_linkedin:\n"
"                                <td style=\"padding-right: 4px;\"><a href=\"${company.social_linkedin}\" style=\"text-decoration:none;\" target=\"_blank\">\n"
"                                  <img src=\"/res_users_signature/static/img/social_linkedin.jpg\" alt=\"linkedin\" style=\"height: 24px; width: 24px; border-radius: 4px; -webkit-border-radius: 4px; -moz-border-radius: 4px;\" width=\"24\" height=\"24\"/></a>\n"
"                                </td>\n"
"                                % endif\n"
"                                % if company.social_twitter:\n"
"                                <td style=\"padding-right: 4px;\"><a href=\"${company.social_twitter}\" style=\"text-decoration:none;\" target=\"_blank\">\n"
"                                  <img src=\"/res_users_signature/static/img/social_twitter.jpg\" alt=\"twitter\" style=\"height: 24px; width: 24px; border-radius: 4px; -webkit-border-radius: 4px; -moz-border-radius: 4px;\" width=\"24\" height=\"24\"/></a>\n"
"                                </td>\n"
"                                % endif\n"
"                                % if company.social_youtube:\n"
"                                <td style=\"padding-right: 4px;\"><a href=\"${company.social_youtube}\" style=\"text-decoration:none;\" target=\"_blank\">\n"
"                                  <img src=\"/res_users_signature/static/img/social_youtube.jpg\" alt=\"youtube\" style=\"height: 24px; width: 24px; border-radius: 4px; -webkit-border-radius: 4px; -moz-border-radius: 4px;\" width=\"24\" height=\"24\"/></a>\n"
"                                </td>\n"
"                                % endif\n"
"                              </tr>\n"
"                            </tbody>\n"
"                          </table>\n"
"                        </td>\n"
"                      </tr>\n"
"                      % endif\n"
"                    </tbody>\n"
"                  </table>\n"
"                </td>\n"
"              </tr>\n"
"            </tbody>\n"
"          </table>\n"
"\n"
"        "
msgstr ""

